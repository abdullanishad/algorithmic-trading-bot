    "            \"ordertype\": ordertype,\n",
    "            \"producttype\": producttype,\n",
    "            \"duration\": \"DAY\",\n",
    "            \"price\": price,\n",
    "            \"triggerprice\":triggerprice, \n",
    "            \"variety\":variety,\n",
    "            \"squareoff\": \"0\",\n",
    "            \"stoploss\": \"0\",\n",
    "            \"quantity\": qty\n",
    "            }\n",
    "    try:\n",
    "        orderId=obj.placeOrder(orderparams)\n",
